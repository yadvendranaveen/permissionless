# Environment Configuration for AutoTrading Analytics Bot

# Server Configuration
NODE_ENV=development
PORT=3000

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Blockchain Configuration
ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_API_KEY
# Alternative RPC URLs:
# ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/YOUR_PROJECT_ID
# ETHEREUM_RPC_URL=https://rpc.ankr.com/eth/YOUR_API_KEY

# API Keys (Optional - for enhanced features)
COINGECKO_API_KEY=your_coingecko_api_key
# Get free API key from: https://www.coingecko.com/en/api/pricing

# Supra Configuration (Future Integration)
SUPRA_RPC_URL=https://your-supra-rpc-url
SUPRA_API_KEY=your_supra_api_key

# Social Media APIs (Optional)
TWITTER_BEARER_TOKEN=your_twitter_bearer_token
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret

# Notification Configuration
DISCORD_WEBHOOK_URL=your_discord_webhook_url
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Performance Configuration
CACHE_TTL=300
ANALYSIS_INTERVAL=30000
AUTOMATION_INTERVAL=15000
WEBSOCKET_UPDATE_INTERVAL=10000

# Security Configuration
API_RATE_LIMIT=100
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=info 